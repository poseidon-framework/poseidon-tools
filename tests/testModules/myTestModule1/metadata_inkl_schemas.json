                "datatype" : {
                    "base": "decimal",
                    "minimum" : 0,
                    "maximum": 1
                },
                "propertyUrl" : "http://w3id.org/poseidon/terms.rdf.jsonld#fracContam"
            }, {
                "name" : "contamNote",
