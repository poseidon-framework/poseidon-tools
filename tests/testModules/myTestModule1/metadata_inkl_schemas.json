        "url" : "sequencing.csv",
        "tableSchema" : {
            "@context" : ["http://www.w3.org/ns/csvw"],
            "dc:conformsTo" : "http://w3id.org/poseidon/terms.rdf.jsonld#SeqTableInfo",
