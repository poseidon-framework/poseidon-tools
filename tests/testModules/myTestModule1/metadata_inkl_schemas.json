                "datatype" : {
                    "base" : "string",
                    "format" : "[a-zA-Z0-9_\\-]+"
                },
                "propertyUrl" : "http://w3id.org/poseidon/terms.rdf.jsonld#groupLabel"
            }, {
                "name" : "IndividualNote",
                "titles" : "IndividualNote",
                "dc:description" : "(Optional) note for the individual",
                "datatype" : "string",
                "propertyUrl" : "http://w3id.org/poseidon/terms.rdf.jsonld#individualNote"
