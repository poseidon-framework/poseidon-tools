                "dc:description" : "BibTEX key(s) for bibliographical source(s)",
                "separator" : ";",
                "datatype" : {
                    "base" : "string",
                    "format" : "[a-zA-Z0-9_\\-]+"
                },
                "propertyUrl" : "http://w3id.org/poseidon/terms.rdf.jsonld#source"
            }],
            "primaryKey" : "SequencingLabID",
            "foreignKeys": [{
                "columnReference": "IndividualID",
                "reference": {
                    "resource": "individuals.csv",
                    "columnReference": "IndividualID"
                }
            }]
        }
    }]
